@model List<AdministrationViewModel>
<div class="d-flex justify-content-center">
    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
        <li class="nav-item me-5" role="presentation">
            @Html.ActionLink("All", "Index", "Administration", new { keyword = "All" }, new { @class = "nav-link btn btn-lg px-5 " + (ViewBag.Keyword == "All" ? "active" : "") })
        </li>
        <li class="nav-item me-5" role="presentation">
            @Html.ActionLink("Active", "Index", "Administration", new { keyword = "Active" }, new { @class = "nav-link btn btn-lg px-5 " + (ViewBag.Keyword == "Active" ? "active" : "") })
        </li>
        <li class="nav-item me-5" role="presentation">
            @Html.ActionLink("Deactivated", "Index", "Administration", new { keyword = "Deactivated" }, new { @class = "nav-link btn btn-lg px-5 " + (ViewBag.Keyword == "Deactivated" ? "active" : "") })
        </li>
    </ul>
</div>
<div class="tab-content" id="pills-tabContent">
    <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab" tabindex="0">
        <table class="table">
            @{
                await Html.RenderPartialAsync("_UsersPartialHeader");
            }
            <tbody>
            @foreach (var entity in Model)
            {
                await Html.RenderPartialAsync("_DataPartial", entity);
            }
            </tbody>
        </table>
    </div>
</div>